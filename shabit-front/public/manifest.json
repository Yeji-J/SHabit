{
  "name": "SHabit",
  "description": "올바른 자세 습관 만들기",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    // 크롬 익스텐션 처음 실행됐을 때 실행되는
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "storage"],
  "action": {
    "default_popup": "extension.html" // popup
  }
}
